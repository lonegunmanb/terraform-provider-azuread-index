func (t thatWithKeyType) IsUuid() pluginsdk.TestCheckFunc {
	return t.MatchesRegex(regexp.MustCompile(`^[A-Fa-f0-9]{8}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{4}-[A-Fa-f0-9]{12}$`))
}
