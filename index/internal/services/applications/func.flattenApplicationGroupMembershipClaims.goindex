func flattenApplicationGroupMembershipClaims(in nullable.Type[string]) []interface{} {
	if in.IsNull() {
		return []interface{}{}
	}

	ret := make([]interface{}, 0)
	for _, claim := range strings.Split(in.GetOrZero(), ",") {
		ret = append(ret, strings.TrimSpace(claim))
	}

	return ret
}
