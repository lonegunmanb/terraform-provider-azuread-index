package github.com/hashicorp/terraform-provider-azuread/internal/services/serviceprincipals/parse
import (
	"fmt"
	"strings"
)
func PasswordID(idString string) (*CredentialId, error) {
	id, err := ObjectSubResourceID(idString, "password")
	if err != nil {
		return nil, fmt.Errorf("unable to parse Password ID: %v", err)
	}

	return &CredentialId{
		ObjectId: id.objectId,
		KeyType:  id.Type,
		KeyId:    id.subId,
	}, nil
}
