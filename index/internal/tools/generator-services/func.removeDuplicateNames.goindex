func removeDuplicateNames(names []string) []string {
	keys := make(map[string]bool)
	uniqueNames := make([]string, 0)

	for _, name := range names {
		if _, value := keys[name]; !value {
			keys[name] = true
			uniqueNames = append(uniqueNames, name)
		}
	}
	return uniqueNames
}
