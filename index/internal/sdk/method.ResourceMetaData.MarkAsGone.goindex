func (rmd ResourceMetaData) MarkAsGone(idFormatter resourceids.Id) error {
	rmd.Logger.Infof("[DEBUG] %s was not found - removing from state", idFormatter)
	rmd.ResourceData.SetId("")
	return nil
}
